{
    "name": "DYMO Labels",
    "tt_name": "module_name",
    "namespace": "DE\\RUB\\DYMOLabelsExternalModule",
    "description": "An external module integrating DYMO label printers with REDCap.",
    "tt_description": "module_desc",
    "documentation": "README.md",
    "tt_documentation": "module_docs",
    "authors": [
        {
            "name": "Günther Rezniczek",
            "email": "guenther.rezniczek@rub.de",
            "institution": "Ruhr-Universität Bochum"
        }
    ],
    "framework-version": 3,
    "permissions": [
        "redcap_module_project_enable",
        "redcap_data_entry_form"
    ],
    "enable-every-page-hooks-on-system-pages": true,
    "links": {
        "project": [
            {
                "id": "setup",
                "name": "DYMO Labels",
                "tt_name": "link_name",
                "icon": "fas fa-tags",
                "url": "setup.php",
                "show-header-and-footer": true
            }
        ]
    },
    "no-auth-pages": [
        "post",
        "public",
        "public-ajax"
    ],
    "action-tags": [
        {
            "key": "dymo-label",
            "name": "@DYMO-LABEL"
        }
    ],
    "system-settings": [
        {
            "key": "system-allow-public",
            "name": "Allow anonymous access to public plugin pages",
            "tt_name": "config_system_allow_public",
            "type": "checkbox"
        },
        {
            "key": "system-enable-post",
            "name": "Allow projects to enable a POST endpoint (see docs)",
            "tt_name": "config_enable_post",
            "type": "checkbox"
        }
    ],
    "project-settings": [
        {
            "key": "js-debug",
            "name": "Output debug information to the browser console",
            "tt_name": "config_jsdebug",
            "type": "checkbox"
        },
        {
            "key": "skip-printing",
            "name": "Simulate printing (i.e. do not actually print)",
            "tt_name": "config_skipprinting",
            "type": "checkbox"
        },
        {
            "key": "allow-public",
            "name": "Allow anonymous access to the public plugin page",
            "tt_name": "config_allow_public",
            "type": "checkbox"
        },
        {
            "key": "enable-post",
            "name": "Enable the POST endpoint (see docs)",
            "tt_name": "config_post",
            "type": "checkbox"
        },
        {
            "key": "show-link",
            "name": "Show the plugin-page link",
            "tt_name": "config_showlink",
            "type": "checkbox"
        },
        {
            "key": "allow-download",
            "name": "Allow users to download label files",
            "tt_name": "config_allow_download",
            "type": "checkbox"
        },
        {
            "key": "allow-autoprint",
            "name": "Allow the auto print flag for public labels",
            "tt_name": "config_allow_auto",
            "type": "checkbox"
        }
    ],
    "compatibility": {
        "php-version-min": "5.6.0",
        "php-version-max": "",
        "redcap-version-min": "10.1.0",
        "redcap-version-max": ""
     }
}